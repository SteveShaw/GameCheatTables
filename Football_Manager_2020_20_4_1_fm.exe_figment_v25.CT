  local rec = findRecord(parent,desc)
  if rec == nil then
    rec = getAddressList().createMemoryRecord()
    rec.Description = desc
    rec.IsGroupHeader = true
    if parent ~= nil then
      rec.appendToEntry(parent)
    end
  end
  return rec
end

