local expectedExecutable = "fm.exe"
local expectedExeVersion = "20.4.1"

local deactivateScript = function()
  enableAutoDisable(memrec, nil)
end
-------------------------------------------
-- Checking CE version
if(getCEVersion() &lt; 7.0) then
  ShowMessage('Sorry. CE version should be 7.0 or above')
  return
end

-------------------------------------------
-- Global LUA Table was loaded
if _G['getAttachedProcessFileName'] == nil then
  ShowMessage('Table Script was not run.  Unable to continue.')
  deactivateScript()
  return
end

-------------------------------------------
-- Checking Selected Process
if(process == nil) then
  ShowMessage('No process was selected')
  deactivateScript()
  return
elseif (string.upper(process) ~= string.upper(expectedExecutable)) then
  ShowMessage('Warning. Expected Process = '..expectedExecutable)
  deactivateScript()
  return
end

-------------------------------------------
-- Patch for CE70
if waitForPDB then --this way older CE versions are unaffected (By Dark Byte)
  waitForPDB()
end

-------------------------------------------
-- Checking Game Version
local exeVersion = getFileVersion(getAttachedProcessFileName())
if exeVersion ~= nil then
  local major = bAnd(bShr(exeVersion,48),0xFFFF)
  local minor = bAnd(bShr(exeVersion,32),0xFFFF)
  local dev   = bAnd(bShr(exeVersion,16),0xFFFF)
  local build = bAnd(exeVersion,0xFFFF)
  local fileVersionString = string.format("%d.%d.%d", major, minor, dev)
  if fileVersionString ~= expectedExeVersion then
    ShowMessage('Expected FM 2020 '..expectedExeVersion..' but found '..fileVersionString)
    deactivateScript()
    return
  end
end
