function enableAutoDisable(memrec, callback)
  local _currec = memrec
  local t = createTimer()
  t.Interval = 100
  t.onTimer = function()
    _currec.Active = false
    if callback ~= nil then
      callback(_currec)
    end
    if t ~= nil then
      t.Enabled = false
      t.destroy()
    end
    t = nil
  end
  t.Enabled = true
end


